<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<!--suppress ALL -->
<GridPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="de.prog2.uwupatch1.contoller.MadeCharacterStageController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>

    </rowConstraints>
    <stylesheets>
        <URL value="@MadeCharacterStyle.css"/>
    </stylesheets>
    <GridPane>
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints vgrow="SOMETIMES"/>
            <RowConstraints vgrow="SOMETIMES"/>
        </rowConstraints>
        <BorderPane prefHeight="100.0" prefWidth="300.0" GridPane.rowIndex="1">
            <left>
                <Button fx:id="start01" mnemonicParsing="false" onAction="#startBackward" text="&lt;-"
                        BorderPane.alignment="CENTER"/>
            </left>
            <right>
                <Button fx:id="start02" mnemonicParsing="false" onAction="#startForward" text="-&gt;"
                        BorderPane.alignment="CENTER"/>
            </right>
            <center>
                <ImageView fx:id="startDeckIcon" fitHeight="80.0" fitWidth="110.0" pickOnBounds="true"
                           preserveRatio="true" BorderPane.alignment="CENTER">
                </ImageView>
            </center>
            <top>
                <Label fx:id="ethnicityLabel" text="Label" BorderPane.alignment="CENTER"/>
            </top>
        </BorderPane>
        <BorderPane prefHeight="100.0" prefWidth="300.0">
            <left>
                <Button mnemonicParsing="false" onAction="#charBackward" text="&lt;-" BorderPane.alignment="CENTER"/>
            </left>
            <right>
                <Button mnemonicParsing="false" onAction="#charForward" text="-&gt;" BorderPane.alignment="CENTER"/>
            </right>
            <center>
                <ImageView fx:id="charDeckIcon" fitHeight="80.0" fitWidth="110.0" pickOnBounds="true"
                           preserveRatio="true" BorderPane.alignment="CENTER">
                </ImageView>
            </center>
            <top>
                <Label fx:id="personalityLabel" text="Label" BorderPane.alignment="CENTER"/>
            </top>
        </BorderPane>
    </GridPane>
    <GridPane GridPane.columnIndex="1">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <ImageView fx:id="deckIcon01" fitHeight="80.0" fitWidth="110.0" pickOnBounds="true" preserveRatio="true"
                   GridPane.columnIndex="2"/>
        <ImageView fx:id="deckIcon02" fitHeight="80.0" fitWidth="110.0" pickOnBounds="true" preserveRatio="true"
                   GridPane.columnIndex="1"/>
        <ImageView fx:id="deckIcon03" fitHeight="80.0" fitWidth="110.0" pickOnBounds="true" preserveRatio="true"
                   GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </GridPane>
</GridPane>
